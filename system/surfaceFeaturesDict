FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeaturesDict;
}

walls
{
    surfaces ("walls.stl");

    includedAngle   45;

    subsetFeatures
    {
    
        insideBox       (-0.001 -0.001 0.0)(0.001 0.001 0.01);
    }
}

